@model IEnumerable<OtoSpaMVC.Models.ContactUs>
@{
    ViewData["Title"] = "Contact Us";
}
<h2>Contact Us</h2>
@*<p>
        <a asp-action="Create" class="btn btn-success"> <span class="fa fa-plus-square" aria-hidden="true"></span> Create New</a>
    </p>*@
<link rel="stylesheet" href="~/css/TestBody.css" />
<div class="newBody">
    <table class="table table-striped table-hover table-condensed">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ContactDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ContactName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ContactHeader)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modleItem => item.ContactDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modleItem => item.ContactName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ContactHeader)
                    </td>
                    <td>
                        <partial name="_listButtons"
                                 view-data='new ViewDataDictionary(this.ViewData){{ "targetId", item.ID }}' />
                        @*<a asp-action="Edit" asp-route-id="@item.ID" class="btn btn-warning" disabled><span class="fa fa-edit?" aria-hidden="true" /></a> |*@
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>